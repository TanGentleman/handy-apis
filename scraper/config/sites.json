{
  "sites": {
    "modal": {
      "name": "Modal",
      "baseUrl": "https://modal.com/docs",
      "defaultPath": "/guide",
      "testPath": "/guide/dicts",
      "mode": "fetch",
      "extractor": "modal",
      "links": {
        "startUrls": [
          "/guide",
          "/reference",
          "/examples"
        ],
        "pattern": "/docs",
        "maxDepth": 1
      },
      "content": {
        "method": "click_copy",
        "clickSequence": [
          {
            "selector": "//button[@title='Copy page']",
            "waitAfter": 500
          },
          {
            "selector": "button.menu-item:has-text('Copy page')",
            "waitAfter": 1000
          }
        ]
      }
    },
    "convex": {
      "name": "Convex",
      "baseUrl": "https://docs.convex.dev",
      "testPath": "/functions/http-actions",
      "mode": "fetch",
      "extractor": "convex",
      "links": {
        "startUrls": [
          "/home"
        ],
        "pattern": "docs.convex.dev/",
        "maxDepth": 2
      },
      "content": {
        "selector": "button[type='button']:has(div:has-text('Copy as Markdown'))",
        "method": "click_copy"
      }
    },
    "datadog": {
      "name": "Datadog Docs",
      "baseUrl": "https://docs.datadoghq.com",
      "testPath": "/tracing/trace_explorer",
      "mode": "fetch",
      "links": {
        "startUrls": [
          ""
        ],
        "pattern": "docs.datadoghq.com/",
        "maxDepth": 3
      },
      "content": {
        "selector": "#mainContent",
        "method": "inner_html",
        "waitUntil": "domcontentloaded"
      }
    },
    "terraform-aws": {
      "name": "Terraform AWS Provider",
      "baseUrl": "https://registry.terraform.io/providers/hashicorp/aws/latest/docs",
      "testPath": "/resources/acm_certificate",
      "mode": "browser",
      "extractor": "terraform",
      "links": {
        "startUrls": [
          ""
        ],
        "pattern": "/providers/hashicorp/aws/latest/docs",
        "waitFor": "#provider-docs-content",
        "waitUntil": "domcontentloaded"
      },
      "content": {
        "selector": "#provider-docs-content",
        "method": "inner_html",
        "waitUntil": "domcontentloaded"
      }
    },
    "cursor": {
      "name": "Cursor",
      "baseUrl": "https://cursor.com/docs",
      "testPath": "/agent/browser",
      "mode": "browser",
      "extractor": "cursor",
      "links": {
        "startUrls": [
          ""
        ],
        "pattern": "/docs/",
        "waitUntil": "networkidle",
        "maxDepth": 1
      },
      "content": {
        "selector": "//button[@type=\"button\"][.//span[text()=\"Copy page\"]]",
        "method": "click_copy"
      }
    },
    "claude-platform": {
      "name": "Claude Platform",
