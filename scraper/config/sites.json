{
  "sites": {
    "modal": {
      "name": "Modal",
      "baseUrl": "https://modal.com/docs",
      "defaultPath": "/guide",
      "mode": "fetch",
      "extractor": "modal",
      "links": {
        "startUrls": ["/guide", "/reference", "/examples"],
        "pattern": "/docs",
        "maxDepth": 1
      },
      "content": {
        "mode": "browser",
        "waitFor": "//button[@title=\"Copy page\"]",
        "selector": "//button[@title=\"Copy page\"]",
        "method": "click_copy"
      }
    },
    "convex": {
      "name": "Convex",
      "baseUrl": "https://docs.convex.dev",
      "mode": "fetch",
      "extractor": "convex",
      "links": {
        "startUrls": ["/home"],
        "pattern": "docs.convex.dev/",
        "maxDepth": 2
      },
      "content": {
        "mode": "browser",
        "waitFor": "button[type='button']:has(div:has-text('Copy as Markdown'))",
        "selector": "button[type='button']:has(div:has-text('Copy as Markdown'))",
        "method": "click_copy"
      }
    },
    "datadog": {
      "name": "Datadog Docs",
      "baseUrl": "https://docs.datadoghq.com",
      "mode": "fetch",
      "links": {
        "startUrls": [""],
        "pattern": "docs.datadoghq.com/",
        "maxDepth": 3
      },
      "content": {
        "mode": "browser",
        "waitFor": "#mainContent",
        "selector": "#mainContent",
        "method": "inner_html",
        "waitUntil": "domcontentloaded"
      }
    },
    "terraform-aws": {
      "name": "Terraform AWS Provider",
      "baseUrl": "https://registry.terraform.io/providers/hashicorp/aws/latest/docs",
      "mode": "browser",
      "extractor": "terraform",
      "links": {
        "startUrls": [""],
        "pattern": "/providers/hashicorp/aws/latest/docs",
        "waitFor": "#provider-docs-content",
        "waitUntil": "domcontentloaded"
      },
      "content": {
        "waitFor": "#provider-docs-content",
        "selector": "#provider-docs-content",
        "method": "inner_html",
        "waitUntil": "domcontentloaded"
      }
    },
    "cursor": {
      "name": "Cursor",
      "baseUrl": "https://cursor.com/docs",
      "mode": "browser",
      "extractor": "cursor",
      "links": {
        "startUrls": [""],
        "pattern": "/docs/",
        "waitUntil": "domcontentloaded",
        "maxDepth": 1
      },
      "content": {
        "mode": "browser",
        "waitFor": "//button[@type=\"button\"][.//span[text()=\"Copy page\"]]",
        "selector": "//button[@type=\"button\"][.//span[text()=\"Copy page\"]]",
        "method": "click_copy"
      }
    },
    "claude-platform": {
      "name": "Claude Platform",
      "baseUrl": "https://platform.claude.com/docs/en",
      "mode": "fetch",
      "links": {
        "startUrls": ["/intro"],
        "pattern": "/docs/en/",
        "maxDepth": 2
      },
      "content": {
        "mode": "browser",
        "waitFor": "//button[.//span[normalize-space(text())=\"Copy page\"]]",
        "selector": "//button[.//span[normalize-space(text())=\"Copy page\"]]",
        "method": "click_copy"
      }
    },
    "claude-code": {
      "name": "Claude Code",
      "baseUrl": "https://code.claude.com/docs/en",
      "mode": "fetch",
      "links": {
        "startUrls": [""],
        "pattern": "/docs/en/",
        "maxDepth": 1
      },
      "content": {
        "mode": "browser",
        "waitFor": "#page-context-menu-button",
        "selector": "#page-context-menu-button",
        "method": "click_copy"
      }
    },
    "unsloth": {
      "name": "Unsloth",
      "baseUrl": "https://unsloth.ai/docs",
      "mode": "fetch",
      "extractor": "unsloth",
      "links": {
        "startUrls": [""],
        "pattern": "/docs/",
        "maxDepth": 1,
        "gotoTimeoutMs": 15000
      },
      "content": {
        "mode": "browser",
        "waitFor": "//button[@aria-label=\"Copy page\"]",
        "selector": "//button[@aria-label=\"Copy page\"]",
        "method": "click_copy"
      }
    },
    "playwright": {
      "name": "Playwright Python",
      "baseUrl": "https://playwright.dev/python/docs",
      "mode": "browser",
      "extractor": "playwright",
      "links": {
        "startUrls": ["/library"],
        "pattern": "/python/docs/"
      },
      "content": {
        "waitFor": "main article .theme-doc-markdown",
        "selector": "main article .theme-doc-markdown",
        "method": "inner_html",
        "waitUntil": "domcontentloaded"
      }
    }
  }
}
